{
  "name": "iprompt",
  "version": "0.2.2",
  "description": "Prompt Management System - Tauri-based desktop and web application",
  "author": "Leo Gray",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "docker compose up -d && docker compose logs -f",
    "dev:web": "vite --host 0.0.0.0 --port 13000 --mode web",
    "dev:tauri": "docker compose --profile tauri up -d tauri-dev && docker compose logs -f tauri-dev",
    "dev:api": "tsx watch src/api/index.ts",
    "build": "tsc && vite build",
    "build:web": "tsc && vite build --mode web",
    "build:tauri": "tauri build",
    "build:api": "tsc",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "tauri": "tauri",
    "i18n:extract": "i18next-parser --config i18next-parser.config.js",
    "i18n:translate": "node src/app/scripts/translate.js"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tailwindcss/typography": "^0.5.16",
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/plugin-dialog": "^2.3.2",
    "@tauri-apps/plugin-fs": "^2.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "immer": "^10.0.3",
    "litellm": "^0.12.0",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.6.1",
    "react-markdown": "^9.1.0",
    "react-router-dom": "^6.21.1",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "serve": "^14.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.7.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "i18next-parser": "^9.3.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.2.2"
  }
}
